{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-cycle": "off",
    "no-extra-semi": 0,
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-undef": "off"
  }
}